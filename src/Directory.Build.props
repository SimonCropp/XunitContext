<?xml version="1.0" encoding="utf-8"?>
<Project>
  <PropertyGroup>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <LangVersion>latest</LangVersion>
    <Version>1.3.0</Version>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Copyright>Copyright $([System.DateTime]::UtcNow.ToString(yyyy)). All rights reserved</Copyright>
    <PackageTags>Logging, xUnit</PackageTags>
    <PackageIconUrl>https://raw.github.com/SimonCropp/XunitLogger/master/icon.png</PackageIconUrl>
    <PackageProjectUrl>http://github.com/SimonCropp/XunitLogger</PackageProjectUrl>
    <PackageOutputPath>..\..\nugets</PackageOutputPath>
    <Description>Extends xUnit to simplify logging.</Description>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <AssemblyOriginatorKeyFile>$(SolutionDir)Key.snk</AssemblyOriginatorKeyFile>
    <SignAssembly>true</SignAssembly>
    <DebugType>embedded</DebugType>
    <DebugSymbols>true</DebugSymbols>
    <NoWarn>CS1591</NoWarn>
  </PropertyGroup>
</Project>